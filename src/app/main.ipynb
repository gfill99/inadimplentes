    "# Consulta para obter a quantidade de clientes inadimplentes por faixa de dívida e cidade\n",
    "faixa_valor_estado = pd.read_sql(\"\"\"\n",
    "    SELECT \n",
    "        c.cidade,\n",
    "        CASE \n",
    "            WHEN i.total_em_atraso BETWEEN 2000 AND 3999.99 THEN 'Entre 2000 e 4000'\n",
    "            WHEN i.total_em_atraso BETWEEN 4000 AND 7999.99 THEN 'Entre 4000 e 8000'\n",
    "            WHEN i.total_em_atraso >= 8000 THEN 'Acima de 8000'\n",
    "            ELSE 'Abaixo de 2000'\n",
    "        END AS Faixa_Devedor,\n",
    "        COUNT(*) AS Quantidade,\n",
    "        COUNT(*) * 100.0 / (SELECT COUNT(*) FROM inadimplencia) AS Percentual\n",
    "    FROM inadimplencia i\n",
    "    INNER JOIN Clientes c ON c.id_cliente = i.id_cliente\n",
    "    GROUP BY c.cidade, Faixa_Devedor\n",
    "    ORDER BY c.cidade, Faixa_Devedor;\n",
    "\"\"\", con)\n",
    "\n",
    "faixa_valor_estado"
   ]
  },
